{
  "name": "cordova.nuxtapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt preview",
    "generate": "nuxt generate && rsync -r --delete dist/ app/www/ && find app/www -name '*.html' -exec sed -i '' 's|/nuxt/|nuxt/|g' {} +",
    "cordova": "cd app && cordova",
    "android": "cd app && cordova build android && cordova run android",
    "ios": "cd app && cordova build ios && cordova run ios"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "cordova": "^12.0.0",
    "nuxt": "^4.1.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@novalumo/biome-config": "^0.0.3",
    "@nuxt/devtools": "latest",
    "@types/node": "^18.7.23",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.16",
    "tailwindcss": "^3",
    "typescript": "^5.0.0"
  }
}
