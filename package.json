{
  "name": "cordova.nuxtapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt preview",
    "generate": "nuxt generate && rsync -r --delete dist/ cordova/www/ && find cordova/www -name '*.html' -exec sed -i '' 's|/nuxt/|nuxt/|g' {} +",
    "cap:build": "nuxt generate && npx cap sync",
    "cap:android": "nuxt generate && npx cap sync && npx cap run android",
    "cap:ios": "nuxt generate && npx cap sync && npx cap run ios",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cordova": "cd cordova && cordova",
    "android": "cd cordova && cordova build android && cordova run android",
    "ios": "cd cordova && cordova build ios && cordova run ios"
  },
  "dependencies": {
    "@capacitor-community/barcode-scanner": "^4.0.1",
    "@capacitor/android": "^7.4.3",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "nuxt": "^4.1.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@novalumo/biome-config": "^0.0.3",
    "@nuxt/devtools": "latest",
    "@types/node": "^18.7.23",
    "autoprefixer": "^10.4.12",
    "postcss": "^8.4.16",
    "tailwindcss": "^3",
    "typescript": "^5.0.0"
  }
}
